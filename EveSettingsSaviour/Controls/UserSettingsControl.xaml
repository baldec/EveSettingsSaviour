<UserControl x:Class="EveSettingsSaviour.Controls.UserSettingsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EveSettingsSaviour.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="390">
    <Grid Margin="2px" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <CheckBox Name="cb_WillOverwrite" Grid.Row="0" Grid.Column="0" Margin="10, 0, 10, 0" VerticalAlignment="Center" HorizontalAlignment="Left"/>
    </Grid>
</UserControl>
